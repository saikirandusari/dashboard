{
  "name": "kubernetes-dashboard",
  "version": "1.8.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubernetes/dashboard.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start:prod": "npm run build && ./$npm_package_config_dashboard_binary_path --kubeconfig $npm_package_config_kubeconfig",
    "start-frontend": "ng serve --aot --proxy-config aio/proxy.conf.json",
    "start-backend": "gulp serve --kubeconfig $npm_package_config_kubeconfig",
    "build": "./aio/scripts/build.sh",
    "build:cross": "./aio/scripts/build.sh -c",
    "build:docker:cross" : "npm run build:cross && gulp docker-image:release:cross",
    "build:docker:head": "npm run build && gulp docker-image:head",
    "build:docker:head:cross": "npm run build:cross && gulp docker-image:head:cross",
    "build-frontend": "npm run clean && ng build --aot --prod --outputPath=$npm_package_config_frontend_build_dir",
    "build-backend": "npm run clean && gulp backend:prod",
    "build-backend:cross": "npm run clean && gulp backend:prod:cross",
    "docker-push:head:cross": "npm run build:docker:head:cross && gulp push-to-docker:head:cross",
    "format": "concurrently \"npm run format-frontend\" \"npm run format-backend\"",
    "format-frontend": "clang-format -i src/app/frontend/**/*.ts aio/**/*.js && gulp format-html",
    "format-backend": "goimports -w src/app/backend",
    "check": "npm run check-licenses && npm run lint && npm run test && npm run e2e",
    "check-licenses": "gulp check-license-headers",
    "lint": "npm run lint-frontend && npm run lint-backend",
    "lint-frontend": "npm run lint-ts && npm run lint-sass",
    "lint-ts": "tslint -c tslint.json --project . -t codeFrame",
    "lint-sass": "./node_modules/sass-lint/bin/sass-lint.js -c .sass-lint.yml 'src/app/frontend/**/*.scss' -v -q",
    "lint-backend": "./aio/scripts/lint-backend.sh",
    "test": "npm run test-frontend && npm run test-backend",
    "test-frontend": "ng test --config aio/karma.conf.js --single-run",
    "test-frontend:watch": "ng test --config aio/karma.conf.js",
    "test-backend": "go test github.com/kubernetes/dashboard/src/app/backend/...",
    "coverage-backend": "./aio/scripts/coverage-backend.sh coverage/go.txt",
    "e2e": "protractor ./aio/protractor.conf.js",
    "clean": "rm -rf .go_workspace .tmp .tools coverage dist",
    "postinstall": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/cdk": "^5.0.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/material": "^5.0.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "@uirouter/angular": "^1.0.0-rc.2",
    "core-js": "^2.5.3",
    "material-design-icons": "^3.0.1",
    "roboto-fontface": "^0.8.0",
    "rxjs": "^5.5.6",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/language-service": "^5.1.3",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.5.8",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "clang-format": "^1.2.2",
    "codelyzer": "~4.0.2",
    "concurrently": "~3.5.1",
    "del": "^3.0.0",
    "google-closure-compiler": "^20180101.0.0",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.1.0",
    "gulp-header-license": "^1.0.9",
    "gulp-license-check": "^1.2.1",
    "gulp-util": "^3.0.8",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.7.5",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "~1.3.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "~0.2.2",
    "lite-server": "2.3.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "protractor": "~5.2.2",
    "q": "latest",
    "raw-loader": "^0.5.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "semver": "^5.4.1",
    "source-map-explorer": "~1.5.0",
    "through2": "^2.0.3",
    "ts-node": "~4.1.0",
    "tslint": "~5.8.0",
    "typescript": "~2.4.2"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  }
}
